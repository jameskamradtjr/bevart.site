                },
                "repulse": {
                  "distance": 200,
                  "duration": 0.4
                },
                "push": {
                  "particles_nb": 4
                },
                "remove": {
                  "particles_nb": 2
                }
              }
            },
            "retina_detect": true
          });
        }
        
        // Add glow accent class to interactive elements
        const interactiveElements = document.querySelectorAll('.feature-item, .price-box, .btn-primary, .btn-secondary, .feature-button, .u-btn-price');
        interactiveElements.forEach(element => {
          element.classList.add('glow-accent', 'interactive-hover');
        });
        
        // Intersection Observer for fade-in animations
        const sections = document.querySelectorAll('.u-section-1, .u-section-2, .u-section-3, .u-section-4, .u-section-5, .u-section-6, .u-section-7, .u-section-8, .u-section-9, .u-section-10, .u-section-11, .u-section-12, .u-section-13, .u-section-14, .u-section-15, .u-section-16, .u-section-17, #sec-6776, #sec-10ca, #carousel_c2fa');
        
        sections.forEach(section => {
          section.classList.add('fade-in-section');
        });
        
        const observerOptions = {
          threshold: 0.1,
          rootMargin: "0px 0px -100px 0px"
        };
        
        const observer = new IntersectionObserver(function(entries, observer) {
          entries.forEach(entry => {
            if (entry.isIntersecting) {
              entry.target.classList.add('active');
              observer.unobserve(entry.target);
            }
          });
        }, observerOptions);
        
        document.querySelectorAll('.fade-in-section').forEach(section => {
          observer.observe(section);
        });
        
        // Add 3D tilt effect to feature boxes
        const tiltElements = document.querySelectorAll('.perspective-container');
        
        tiltElements.forEach(element => {
          element.addEventListener('mousemove', function(e) {
            const box = this.getBoundingClientRect();
            const mouseX = e.clientX - box.left;
            const mouseY = e.clientY - box.top;
            
            const centerX = box.width / 2;
            const centerY = box.height / 2;
            
            const percentX = (mouseX - centerX) / centerX;
            const percentY = (mouseY - centerY) / centerY;
            
            const image = this.querySelector('.perspective-image');
            const shadow = this.querySelector('.perspective-shadow');
            
            if (image && shadow) {
              image.style.transform = `perspective(1000px) rotateY(${percentX * 5}deg) rotateX(${-percentY * 5}deg)`;
              shadow.style.transform = `perspective(1000px) rotateY(${percentX * 5}deg) rotateX(${-percentY * 5}deg) translateZ(-50px)`;
            }
          });
          
          element.addEventListener('mouseleave', function() {
            const image = this.querySelector('.perspective-image');
            const shadow = this.querySelector('.perspective-shadow');
            
            if (image && shadow) {
              image.style.transform = 'perspective(1000px) rotateY(-10deg) rotateX(5deg)';
              shadow.style.transform = 'perspective(1000px) rotateY(-10deg) rotateX(5deg) translateZ(-50px)';
            }
          });
        });
      });
    </script>
    
