    <script>
      document.addEventListener('DOMContentLoaded', function() {
        // Initialize particles.js
        if (typeof particlesJS !== 'undefined') {
          particlesJS("particles-js", {
            "particles": {
              "number": {
                "value": 80,
                "density": {
                  "enable": true,
                  "value_area": 800
                }
              },
