        color: #ffffff;
      }
      
      .element-1 {
        top: 20%;
        left: -10%;
        animation-delay: 0s;
      }
      
      .element-2 {
        top: 45%;
        right: -10%;
        animation-delay: 1s;
      }
      
      .element-3 {
        bottom: 20%;
        left: 20%;
        animation-delay: 2s;
      }
      
      @keyframes floatElement {
        0% {
          transform: translateY(0);
        }
        50% {
          transform: translateY(-15px);
        }
        100% {
